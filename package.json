{
  "name": "@fingerprintjs/fingerprintjs-pro-react-native",
  "version": "1.0.4",
  "description": "Official React Native client for FingerprintJS PRO. Best identification solution for React Native.",
  "main": "./build/fpjs-pro-react-native.cjs.js",
  "module": "./build/fpjs-pro-react-native.esm.js",
  "types": "./build/fpjs-pro-react-native.d.ts",
  "scripts": {
    "build": "rm -rf ./build && rollup -c rollup.config.js",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:dts": "tsc -p check.tsconfig.json",
    "release:major": "changelog -M && git add CHANGELOG.md && yarn version --major --no-git-tag-version",
    "release:minor": "changelog -m && git add CHANGELOG.md && yarn version --minor --no-git-tag-version",
    "release:patch": "changelog -p && git add CHANGELOG.md && yarn version --patch --no-git-tag-version"
  },
  "files": [
    "android",
    "ios",
    "RNFingerprintjsPro.podspec",
    "build"
  ],
  "keywords": [
    "fraud",
    "fraud detection",
    "fraud prevention",
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "browser fingerprint",
    "device fingerprint",
    "privacy"
  ],
  "author": "FingerprintJS, Inc (https://fingerprint.com)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "*",
    "@types/react-native": "*",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-native": "^4.0.0",
    "generate-changelog": "^1.8.0",
    "jest": "^27.4.1",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.71.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-license": "^2.7.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
